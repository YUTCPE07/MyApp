<button class="btn btn-success">test</button>
<button class="btn btn-info">test</button>
<button class="btn btn-warning">test</button>
<button class="btn btn-danger">test</button>
<button class="btn btn-default">test</button>
<button class="btn btn-primary">test</button>

<div class="row">
    <div class="col-xs-12">
        <div class="col-xs-6 col-md-12">

        </div>
        <div class="col-xs-6 col-md-12">
                <div class="form-group">
                    <label for="exampleInputEmail1">Email address</label>
                    <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Email">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Password</label>
                    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
                </div>
                <button id="submit" class="btn btn-default"><span class="glyphicon glyphicon-search" aria-hidden="true"></span> Submit</button>
            <div>
                Result : <p id="result"></p>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function() {
        $("#submit").click(function () {
            var email = $("#exampleInputEmail1").val();
            var password = $("#exampleInputPassword1").val();
            $.get("/getdata", {email: email, pass: password})
                    .done(function (data) {
                        $("#result").text(data);
                    });
        });

        $("#exampleInputEmail1").keypress(function() {
            var email = $("#exampleInputEmail1").val();
            $("#result").text(email);
        });
    });
</script>

